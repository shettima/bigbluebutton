<?xml version="1.0" encoding="utf-8"?>
<mx:Application xmlns:mx="http://www.adobe.com/2006/mxml" 
	xmlns:co="org.bigbluebutton.main.view.components.com.containers.*" backgroundAlpha="0"
	layout="vertical">
	
	<mx:Script>
		<![CDATA[
			import org.bigbluebutton.main.view.components.com.ui.IBSdialog;
			import org.bigbluebutton.main.view.components.com.ui.IBSNavigator;
			import org.bigbluebutton.main.view.components.com.ui.IBSMenu;
		]]>
	</mx:Script>
	
	<mx:Style source="org/bigbluebutton/main/view/components/style.css" />


		<mx:VDividedBox width="100%" height="100%">

			<mx:HDividedBox width="100%" height="100%">
				<mx:Canvas width="100%" height="100%" id="presentCavas">
			
				</mx:Canvas>

				
				<co:IBSDockBox width="250" height="100%" id="viewersList"
					position="right" 
					dockPanelFactories="{[IBSNavigator,IBSMenu]}"
					selectedIndex="0"
					panelStyleName="dockablePanel" />
			</mx:HDividedBox>
			<co:IBSDockBox width="100%" height="225" id="presentDiabox"
				position="bottom" 
				dockPanelFactories="{[IBSdialog]}"
				selectedIndex="1" 
				panelStyleName="dockablePanel" />
		</mx:VDividedBox>

</mx:Application>
